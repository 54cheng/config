server:
  port: 80
  
spring:
    active:
    - app
    eureka: 
     client: 
       register-with-eureka: false #获取服务提供方地址,本身不注册进eureka服务
       service-url: 
         defaultZone: http://eureka7001.com:7001/eureka/
    
---
